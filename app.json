{
  "expo": {
    "name": "Eternal Chapter",
    "slug": "eternal-chapter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eternalchapter",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eternalchapter.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#D1D3D4",
        "foregroundImage": "./assets/images/icon.png"
      },
      "package": "com.eternalchapter.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#D1D3D4"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7882e9c8-5aea-49e3-ac04-4dd88dce4f76"
      }
    }
  }
}